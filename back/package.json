{
  "name": "lepanivore-website-backend",
  "version": "1.11.0",
  "private": true,
  "scripts": {
    "postinstall": "[[ -n $APP_FOLDER ]] && npm run build && npm run typeorm:migration:run || exit 0",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:prod",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "rimraf .stryker-tmp && npm run test:cleanArchitecture && npm run lint",
    "pretest:e2e": "rimraf e2e.sqlite",
    "test": "npm run test:cov && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config e2e/jest-e2e.json --runInBand",
    "test:cleanArchitecture": "./tools/check-clean-architecture.sh",
    "test:mutation": "stryker run",
    "pretypeorm": "ts-node -r tsconfig-paths/register database/write-ormconfig-for-migrations.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:run": "npm run typeorm -- migration:run -c schema"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/common": "8.4.4",
    "@nestjs/core": "8.4.4",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.4.4",
    "@nestjs/typeorm": "8.0.3",
    "@sentry/node": "6.19.7",
    "bcrypt": "5.0.1",
    "helmet": "5.0.2",
    "joi": "17.6.0",
    "json2csv": "5.0.7",
    "lodash": "4.17.21",
    "nest-raven": "8.1.0",
    "nodemailer": "6.7.4",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.7.3",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "sqlite3": "5.0.6",
    "typeorm": "0.2.45"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.5",
    "@nestjs/schematics": "8.0.10",
    "@nestjs/testing": "8.4.4",
    "@stryker-mutator/core": "6.0.0",
    "@stryker-mutator/jest-runner": "6.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/json2csv": "5.0.3",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.31",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.12",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.0",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tsc-watch": "5.0.3",
    "tsconfig-paths": "4.0.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "4.6.4"
  },
  "engines": {
    "node": "~16.15.0",
    "npm": "~8.8"
  }
}
